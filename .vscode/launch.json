    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "com.entsport.App",
            "projectName": "ent_sport"
        },
        {
            "type": "java",
            "name": "Allrunner",
            "request": "launch",
            "mainClass": "entSport.Allrunner",
            "projectName": "ent_sport"
        },
        {
            "type": "java",
            "name": "dummy",
            "request": "launch",
            "mainClass": "entSport.dummy",
            "projectName": "ent_sport"
        },
        {
            "type": "java",
            "name": "getOtpUsingEmail",
            "request": "launch",
            "mainClass": "utility.getOtpUsingEmail",
            "projectName": "ent_sport"
        }
    ]
